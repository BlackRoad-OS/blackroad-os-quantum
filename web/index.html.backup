    <script>
        // Animate on load
        window.addEventListener('load', () => {
            const bars = document.querySelectorAll('.bar-fill');
            bars.forEach((bar, index) => {
                setTimeout(() => {
                    bar.style.width = bar.style.width;
                }, index * 100);
            });
        });

        // Live status blink
